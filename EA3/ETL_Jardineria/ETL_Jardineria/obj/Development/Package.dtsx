INNER JOIN DestinoTBGeografiaST G ON C.ciudad=G.ciudad AND C.codigo_postal=G.codigo_postal AND C.pais=G.pais AND C.region=G.region
INNER JOIN TBEmpleadoST E ON C.ID_empleado_rep_ventas=E.ID_empleado</property>
