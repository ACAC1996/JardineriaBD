                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT E.ID_empleado,E.ID_empleado_O,(ISNULL(E.nombre,' ')+' '+ISNULL(E.apellido1,' ')+' '+ISNULL(E.apellido2, ' ')) as nombre,E.extension,E.email, O.Descripcion as oficina,
